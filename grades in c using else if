#include<stdio.h>
#include<conio.h>
void main()
{
int m;
printf("enter marks");
scanf("%d",m);
if(m<=100||m>=85)
{
     printf("Grade A");
}
elseif(m<=84||m>=70)
{
     printf("Grade B");
}
elseif(m<=69||m>=55)
{
     printf("Grade C");
}
elseif(m<=54||m>=40)
{
     printf("Grade D");
}
else
{ 
printf("Grade F");
}
getch();
}


